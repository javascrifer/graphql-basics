    "dev": "tsnd --respawn src/index.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json -t stylish --fix",
    "format": "prettier --write src/**/*.ts && git add src"
  },
  "engines": {
    "node": "13.12.0"
  },
  "author": "Nikolajus Lebedenko",
  "license": "MIT",
  "dependencies": {
    "graphql-yoga": "^1.18.3",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/node": "^13.9.3",
    "@types/uuid": "^7.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.1",
    "ts-node": "^8.8.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "pre-commit": [
    "build",
    "lint",
    "format"
